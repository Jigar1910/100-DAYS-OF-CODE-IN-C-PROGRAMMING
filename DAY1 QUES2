// Write a program to input two numbers and display their sum, difference, product, and quotient.
#include <stdio.h>
int main() {
    int num1,num2;
    printf("Enter num1:");
    scanf("%d",&num1);
    printf("Enter num2:");
    scanf("%d",&num2);
    int sum=num1+num2;
    int diff=num1-num2;
    int mul=num1*num2;
    int divi=num2/num1;
    printf("The sum of 2 numbers is:%d",sum);
    printf("\nThe difference of 2 numbers is:%d",diff);
    printf("\nThe multiplication of 2 numbers is:%d",mul);
    printf("\nThe division of 2 numbers is:%d",divi);
    
    

    return 0;
}
