// WAP to calculate factorial of numbers.
#include <stdio.h>

int main() {
    int n,factorial=1;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        factorial=factorial*i;
    }
    printf("The value of factorial of %d numbers is %d\n",n,factorial);

    return 0;
}
