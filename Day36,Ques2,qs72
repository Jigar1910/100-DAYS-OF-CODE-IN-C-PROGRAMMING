// Find the sum of all elements in a matrix.
#include <stdio.h>

int main() {
    int r,c,sum=0;
    printf("Enter the rows and columns:");
    scanf("%d%d",&r,&c);
    int arr[r][c];
    printf("Enter the elements:");
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            scanf("%d",&arr[i][j]);
            sum+=arr[i][j];
        }
    }
    printf("The sum of this matrix is:%d\n",sum);
return 0;
}
