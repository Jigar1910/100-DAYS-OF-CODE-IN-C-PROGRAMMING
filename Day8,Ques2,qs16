#include <stdio.h>

int main() {
    int x,y,z;
    printf("Enter the value of x,y,z:");
    scanf("%f%f%f",&x,&y,&z);
    if(x>y){
        if(x>z){
            printf("x is the largest among three:");
        }else{
            printf("z is the largest among three");
        }
    
    }
    if(y>x){
        if(y>z){
            printf("y is largest among 3:");
        }else{
            printf("z is the largest among three:");
        }
    }

    
    
    
    return 0;
}
