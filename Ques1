// Ques1. Write a program to input two numbers and display their sum.
#include <stdio.h>

int main() {
    int num1,num2;
    printf("Enter num1:");
    scanf("%d",&num1);
    printf("Enter num2:");
    scanf("%d",&num2);
    int sum=num1+num2;
    printf("The sum of 2 numbers is:%d",sum);
    

    return 0;
}
