// WAP to print product of even numbers from 1 to n.
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,product=1;
    printf("Enter the value of n:");
    scanf("%d",&n);
    for(int i=2;i<=n;i=i+2){
        product=product*i;
    }
    printf("Product of even n numbers from 1 to %d is %d\n",n,product);

    return 0;
}
