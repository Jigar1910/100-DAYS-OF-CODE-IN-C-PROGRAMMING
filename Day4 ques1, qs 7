// Ques7.Write a program to swap two numbers without using a third variable.
#include <stdio.h>

int main() {
    float A,B;
    printf("Enter the values of A and B:");
    scanf("%f%f",&A,&B);
    
    A=A-B;
    B=A-B;
    A=A-B;
    
    printf("After swapping A and B:");
    printf("\nA=%f",A);
    printf("\nB=%f",B);
    
    return 0;
}
