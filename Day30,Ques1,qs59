//Count even and odd numbers in an array.
#include <stdio.h>

int main() {
    int n,i,even=0,odd=0;
    printf("Enter the value of n:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
        if(arr[i]%2==0){
            even++;
        } else{
            odd++;
        }
    }
    printf("Even=%d,Odd=%d\n",even,odd);

    return 0;
}
